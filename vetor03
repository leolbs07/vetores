#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <locale.h>
#include <limits.h>

#define TAM 3

int main() {
    setlocale(LC_ALL, "");
    
    int vetor [TAM];
    int i;
    
    printf("==== CONDIÇÕES ====\n");
    for (i = 0; i < TAM; i++) {
	
    	printf("Digite o valor da nota %d: ", i + 1);
    	scanf("%d", &vetor[i]);
	}
	
	return 0;
	
}
