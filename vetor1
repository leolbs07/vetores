#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <locale.h>
#include <limits.h>

int main() {
    setlocale(LC_ALL, "");
    
    int numero [5];
    numero [0] = 1;
    numero [1] = 2;
    numero [2] = 3;
    numero [3] = 4;
    numero [4] = 5;
    
    printf("Elemento 1: %d\n", numero[0]);
    printf("Elemento 2: %d\n", numero[1]);
    printf("Elemento 3: %d\n", numero[2]);
    printf("Elemento 4: %d\n", numero[3]);
    printf("Elemento 5: %d\n", numero[4]);
    
}
